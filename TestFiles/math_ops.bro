$ Math Functions test

func main() {


int a = 500;
string b = "400";
real c = 5.3;


$ Addition Tests

print("Addition Tests");

print(a + a); 	$ int + int

print(b + b);	$ string + string

print(c + c);	$ real + real

print(a + b);	$ int + string

print(b + a);	$ string + int

print(a + c);	$ int + real

print(c + a);	$ real + int


print("");


$ Subtraction Tests

print("Subtraction Tests");

int a = 34;
real c = 45.3;

print(a - a);	$ int - int

print(c - c);	$ real - real

print(a - c);	$ int - real

print(c - a);	$ real - int


print("");



$ Multiplication Tests

print("Multiplication Tests");

print(a * a);	$ int * int

print(c * c);	$ real * real

print(a * c);	$ int * real

print(c * a);	$ real * int

print("");



$ Division Tests

print("Division Tests");

print(a / a);	$ int / int

print(c / c);	$ real / real

print(a / c);	$ int / real

print(c / a);	$ real / int

print("");



$ Integer Division Tests

print("Integer Division Tests");

print(a // a);	$ int // int

print(c // c);	$ real // real

print(a // c);	$ int // real

print(c // a);	$ real // int

print("");

}

main()